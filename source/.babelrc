{
  "presets":[
    ["env",
      {
        "modules":false,
        "debug":false,
        "useBuiltIns":true,
        "targets":{
          "browsers":"> 1%"
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    "babel-plugin-transform-class-properties"
  ],
  "env":{
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}